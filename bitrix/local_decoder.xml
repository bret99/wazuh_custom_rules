</decoder>

<!-- MP4 downloaded -->
<decoder name="portal_download_mp4">
  <prematch>^\d+.\d+.\d+.\d+ - - \S+ \S+ - \S+ 2\d+ "GET \Sdisk\SdownloadFile\S\d+\S\S+.mp\S+ \.+</prematch>
</decoder>

<!-- Archive downloaded -->
<decoder name="portal_download_archive">
  <prematch>^\d+.\d+.\d+.\d+ - - \S+ \S+ - \S+ 2\d+ "GET \Sbitrix\Sservices\Smain\Sajax.php\Saction=disk.api.folder.downloadArchive\S+ \.+</prematch>
</decoder>

<!-- Bitrix portal downloadings childs -->
  <!-- Archives -->
<decoder name="portal_download_archive_child">
  <parent>portal_rtmis_download_archive</parent>
  <regex>(\d+.\d+.\d+.\d+) - - \S+ \S+ - \S+ 2\d+ "GET (\Sbitrix\Sservices\Smain\Sajax.php\Saction=disk.api.folder.downloadArchive\S+) \.+</regex>
  <order>srcip,url</order>
</decoder>

  <!-- PPTX -->
<decoder name="portal_download_pptx_child">
  <parent>portal_rtmis_download_pptx</parent>
  <regex>(\d+.\d+.\d+.\d+) - - \S+ \S+ - \S+ 2\d+ "GET (\Sdisk\SdownloadFile\S\d+\S\S+.pptx)\.+</regex>
  <order>srcip,url</order>
</decoder>

