  <prematch>^\d+.\d+.\d+.\d+ - - \S+ \S+ - \S+ 2\d+ "GET \Sdisk\SdownloadFile\S\d+\S\S+.xlsx\.+</prematch>
</decoder>

<!-- PPTX downloaded -->
