<!-- Bitrix portal downloadings -->
<!-- DOCX downloaded -->
<decoder name="portal_download_docx">
  <prematch>^\d+.\d+.\d+.\d+ - - \S+ \S+ - \S+ 2\d+ "GET \Sdisk\SdownloadFile\S\d+\S\S+.docx\.+</prematch>
</decoder>

<!-- PDF downloaded -->
<decoder name="portal_download_pdf">
  <prematch>^\d+.\d+.\d+.\d+ - - \S+ \S+ - \S+ 2\d+ "GET \Sdisk\SdownloadFile\S\d+\S\S+.pdf\.+</prematch>
</decoder>

<!-- XLSX downloaded -->
