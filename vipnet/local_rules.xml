  <rule id="100113" level="7">
    <decoded_as>vipnet_rvpn_shell_1</decoded_as>
    <description>Vipnet: [rvpnshell] $(vipnet.cmd_type) Command $(vipnet.cmd_command) run.</description>
  </rule>

