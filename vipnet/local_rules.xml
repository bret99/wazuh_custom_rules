<!-- Vipnet rules [sshd, webguifcgi-server] -->
<group name="vipnet,">
  <rule id="100101" level="7">
    <decoded_as>vipnet_sshd_1</decoded_as>
    <description>Vipnet: [sshd] $(password_state) password for $(dstuser) from $(srcip) port $(dstport).</description>
  </rule>

  <rule id="100102" level="7">
    <decoded_as>vipnet_sshd_2</decoded_as>
    <description>Vipnet: [sshd] session $(vipnet.ssh_session) for user $(dstuser).</description>
  </rule>

  <rule id="100103" level="5">
    <decoded_as>vipnet_sshd_3</decoded_as>
    <description>Vipnet: [sshd] session $(vipnet.ssh_session) for user $(dstuser).</description>
  </rule>

  <rule id="100104" level="7">
    <decoded_as>vipnet_webgui_1</decoded_as>
    <description>Vipnet: [webgui-fcgi] user from $(srcip) is $(vipnet.webgui_session).</description>
  </rule>

  <rule id="100105" level="7">
    <decoded_as>vipnet_webgui_2</decoded_as>
    <description>Vipnet: [webgui-fcgi] user from $(srcip) is logged in as admin.</description>
  </rule>

  <rule id="100106" level="7">
    <decoded_as>vipnet_iplirpasswd_1</decoded_as>
